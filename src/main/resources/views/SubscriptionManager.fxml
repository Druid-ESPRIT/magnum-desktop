<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import java.lang.String?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="720.0" prefWidth="1280.0"
            xmlns="http://javafx.com/javafx/8.0.171" fx:controller="com.druid.controllers.SubscriptionManagerController">
    <children>
        <BorderPane prefHeight="720.0" prefWidth="1282.0">
            <left>
                <VBox prefHeight="720.0" prefWidth="288.0" styleClass="right-bar" stylesheets="@../style.css"
                      BorderPane.alignment="CENTER">
                    <children>
                        <HBox prefHeight="112.0" prefWidth="288.0"/>
                        <HBox alignment="CENTER" prefHeight="58.0" prefWidth="245.0" stylesheets="@../style.css">
                            <children>
                                <TextField fx:id="tfsearch" prefHeight="30.0" prefWidth="140.0"
                                           promptText="Type here to search" styleClass="transparent"
                                           stylesheets="@../style.css">
                                    <HBox.margin>
                                        <Insets left="20.0"/>
                                    </HBox.margin>
                                    <font>
                                        <Font name="Cambria" size="14.0"/>
                                    </font>
                                </TextField>
                                <Button mnemonicParsing="false" onAction="#SearchClicked" styleClass="search-button"
                                        stylesheets="@../style.css" text="Search" textFill="WHITE"/>
                            </children>
                            <VBox.margin>
                                <Insets left="20.0" right="20.0"/>
                            </VBox.margin>
                            <styleClass>
                                <String fx:value="search-box"/>
                                <String fx:value="shadow"/>
                            </styleClass>
                        </HBox>
                        <VBox prefHeight="30.0" prefWidth="288.0"/>
                        <VBox prefHeight="77.0" prefWidth="288.0"/>
                        <Label fx:id="chid"/>
                    </children>
                </VBox>
            </left>
            <center>
                <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
                    <children>
                        <HBox prefHeight="100.0" prefWidth="200.0"/>
                        <HBox alignment="CENTER" prefHeight="623.0" prefWidth="994.0">
                            <children>
                                <ScrollPane prefHeight="600.0" prefWidth="920.0" styleClass="transparent"
                                            stylesheets="@../style.css">
                                    <content>
                                        <GridPane fx:id="grid">
                                            <rowConstraints>
                                                <RowConstraints minHeight="10.0" prefHeight="30.0"/>
                                            </rowConstraints>
                                        </GridPane>
                                    </content>
                                    <HBox.margin>
                                        <Insets bottom="10.0" top="10.0"/>
                                    </HBox.margin>
                                </ScrollPane>
                            </children>
                        </HBox>
                    </children>
                </VBox>
            </center>
        </BorderPane>
    </children>
</AnchorPane>
